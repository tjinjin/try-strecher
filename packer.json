{
  "builders": [{
    "type": "amazon-ebs",
    "region": "ap-northeast-1",
    "source_ami": "ami-b80b6db8",
    "instance_type": "t2.micro",
    "ssh_username": "centos",
    "ssh_timeout": "5m",
    "ami_name": "packer-example {{timestamp}}"
  }],
 "provisioners": [{
    "type": "chef-solo",
