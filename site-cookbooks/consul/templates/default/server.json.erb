{
  "server": true,
  "data_dir": "/tmp/consul",
<% if @environment == 'development' %>
  "node_name": "<%= @node_name %>",
  "bind_addr": "<%= @bind_addr %>",
<% end %>
  "client_addr": "0.0.0.0",
  "bootstrap_expect": 3,
  "start_join": ["<%= @leader_addr %>"],
  "watches": [
    {
      "type": "event",
      "name": "example_deploy",
      "handler": "stretcher"
    }
  ]
}
