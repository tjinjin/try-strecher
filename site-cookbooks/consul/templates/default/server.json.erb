{
  "server": <%= @consul_mode %>,
  "data_dir": "/tmp/consul",
<% if @environment == 'development' %>
  "node_name": "<%= @node_name %>",
  "bind_addr": "<%= @bind_addr %>",
  "log_level": "DEBUG",
<% end %>
  "client_addr": "0.0.0.0",
  "bootstrap_expect": 3,
  "watches": [
    {
      "type": "event",
      "name": "deploy_<%= @consul_role %>_development",
      "handler": "stretcher"
    }
  ]
}
